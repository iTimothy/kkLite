<view class="wrap tel_wrap">
	<input maxlength="11" placeholder="请输入手机号或者快快账号" placeholder-class="ph" confirm-type="done" bindconfirm="login" type="number" bindinput="inputHandle" id="tel"/>
</view>
<view class="wrap pw_wrap">
	<input maxlength="16" placeholder="请输入登录密码" placeholder-class="ph" type="password" confirm-type="done" bindconfirm="login"     bindinput="inputHandle" id="pw"/>
</view>
<view class="wrap login_wrap">
	<button catchtap="login" class="login_btn" hover-class="btn_hover">登录</button>
</view>
